<launch>
  
  <node pkg="yolopkg" type="yolo_detector_node" name="yolo_detector" output="screen">
    
    <param name="model_path" value="$(find yolopkg)/best_openvino_model/best"/>
    <param name="conf_threshold" value="0.5"/>
    <param name="cls_threshold" value="0.75"/>
    <param name="iou_threshold" value="0.5"/>
    <param name="way" value="0"/> <!--0: 发送最佳检测结果  1：发送阈值以上的检测结果-->
   
    <param name="image_topic" value="/camera/map_server"/>
    
    <param name="queue_size" value="2"/>
    <param name="num_threads" value="2"/>
  </node> 

</launch>